                {
                    "_postman_id": "859e5bf3-d86c-4174-bbc9-e73f168cb326",
                    "name": "01 Authorize Code Grant Access Token",
                    "event": [
                        {
                            "listen": "test",
                            "script": {
                                "type": "text/javascript",
                                "exec": "var jsonData = JSON.parse(responseBody);\npostman.setEnvironmentVariable(\"accessToken\", jsonData.access_token);\nvar jsonData = JSON.parse(responseBody);\npostman.setEnvironmentVariable(\"refreshToken\", jsonData.refresh_token);"
                            }
                        }
                    ],
                    "request": {
                        "url": "https://{{hostenv}}/oauth/token",
                        "method": "POST",
                        "header": [
                            {
                                "key": "Authorization",
                                "value": "Basic {{encodedKeys}}",
                                "description": ""
                            }
                        ],
                        "body": {
                            "mode": "urlencoded",
                            "urlencoded": [
                                {
                                    "key": "code",
                                    "value": "{{codeFromUrl}}"
                                },
                                {
                                    "key": "grant_type",
                                    "value": "authorization_code"
                                }
                            ]
                        }
                    },
                    "response": []
                },
                {
                    "_postman_id": "d0f03be5-2340-4210-a08f-dbebffb5dd1e",
                    "name": "02 JWT Access Token",
                    "event": [
                        {
                            "listen": "test",
                            "script": {
                                "type": "text/javascript",
                                "exec": "var jsonData = JSON.parse(responseBody);\npostman.setEnvironmentVariable(\"accessToken\", jsonData.access_token);\nvar jsonData = JSON.parse(responseBody);\npostman.setEnvironmentVariable(\"refreshToken\", jsonData.refresh_token);"
                            }
                        }
                    ],
                    "request": {
                        "url": "https://{{hostenv}}/oauth/token",
                        "method": "POST",
                        "header": [
                            {
                                "key": "Authorization",
                                "value": "Basic {{encodedKeys}}",
                                "description": ""
                            }
                        ],
                        "body": {
                            "mode": "urlencoded",
                            "urlencoded": [
                                {
                                    "key": "assertion",
                                    "value": "{{assertionHere}}"
                                },
                                {
                                    "key": "grant_type",
                                    "value": "urn:ietf:params:oauth:grant-type:jwt-bearer"
                                }
                            ]
                        }
                    },
                    "response": []
                },
                {
                    "_postman_id": "37e48121-3b14-4deb-a5be-df2efed40e84",
                    "name": "03 Refresh Access Token",
                    "event": [
                        {
                            "listen": "test",
                            "script": {
                                "type": "text/javascript",
                                "exec": "var jsonData = JSON.parse(responseBody);\npostman.setEnvironmentVariable(\"accessToken\", jsonData.access_token);\nvar jsonData = JSON.parse(responseBody);\npostman.setEnvironmentVariable(\"refreshToken\", jsonData.refresh_token);"
                            }
                        }
                    ],
                    "request": {
                        "url": "https://{{hostenv}}/oauth/token",
                        "method": "POST",
                        "header": [
                            {
                                "key": "Authorization",
                                "value": "Basic {{encodedKeys}}",
                                "description": ""
                            }
                        ],
                        "body": {
                            "mode": "urlencoded",
                            "urlencoded": [
                                {
                                    "key": "refresh_token",
