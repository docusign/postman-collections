				{
					"name": "01 Authorize Code Grant Access Token",
					"event": [
						{
							"listen": "test",
							"script": {
								"type": "text/javascript",
								"exec": [
									"var jsonData = JSON.parse(responseBody);",
									"postman.setEnvironmentVariable(\"accessToken\", jsonData.access_token);",
									"var jsonData = JSON.parse(responseBody);",
									"postman.setEnvironmentVariable(\"refreshToken\", jsonData.refresh_token);"
								],
								"id": "639b4332-bb53-4167-b851-faee8b90ba58"
							}
						}
					],
					"id": "11add7c8-d2e2-471d-a87d-cabb09122e74",
					"protocolProfileBehavior": {
						"disableBodyPruning": true
					},
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Authorization",
								"value": "Basic {{encodedKeys}}"
							}
						],
						"body": {
							"mode": "urlencoded",
							"urlencoded": [
								{
									"key": "code",
									"value": "{{codeFromUrl}}"
								},
								{
